mixin transaction(data={})
    table.text-center
        tr
            td(style = "width:60%").p-3
                a(href = "") #{data.description}
            
            td.text-center.p-3(style = 'width:10%') #{data.seller}

            td.text-center.p-3(style = 'width:20%') #{data.createdAt}

            td.text-center.p-3(style = 'width:10%') #{data.amount + `kw`} 
